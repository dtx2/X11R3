   0xfe, 0xf7, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff,
   0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff,
   0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff,
   0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff,
   0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff,
   0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff,
   0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff
};

Pixmap    pm;
Display   *dpy_;
GC        gc;
XGCValues gcv;

XSetWindowAttributes newWindow = {
    ParentRelative,   /* background pixmap */
    0,                /* background pixel */
    None,             /* border pixmap */
    0,                /* border color */
    ForgetGravity,    /* bit gravity */
    ForgetGravity,    /* window gravity */
    NotUseful,        /* backing store */
    AllPlanes,        /* backing planes */
    0,                /* backing pixel */
    False,            /* save under */
    ExposureMask      /* event mask */
	| ButtonPressMask,
    ExposureMask,     /* not-propagated event mask */
    False,             /* override redirect */
    CopyFromParent,   /* colormap */
    None              /* cursor */
};


main()
{
    int		  nplanes;
    Colormap 	  cmap;
    XColor	  cdef;
    unsigned long planemasks[5];
    unsigned long pixels[8];
    unsigned long color0;
    unsigned long color1;
    unsigned long color2;
    int           i;
    Window        w;
    XEvent        event;
    
    if(!(dpy_ = XOpenDisplay(""))){
	fprintf(stderr,"can not open display\n");
	exit(1);
    }

    if((nplanes=DefaultDepth(dpy_,DefaultScreen(dpy_))) > 2){
	cmap = DefaultColormap(dpy_,DefaultScreen(dpy_));
	XAllocColorCells(dpy_,cmap,False,planemasks,1,pixels,3);
	color0 = planemasks[0] | pixels[0];
	color1 = planemasks[0] | pixels[1];
	color2 = planemasks[0] | pixels[2];
	cdef.pixel = color0;
	cdef.red = 0xFFFF;
	cdef.blue = 0x0000;
	cdef.green = 0x0000;
	cdef.flags = DoRed|DoGreen|DoBlue;
	XStoreColor(dpy_,cmap,&cdef);
	cdef.pixel = color1;
	cdef.red = 0x0000;
	cdef.blue = 0xFFFF;
	cdef.green = 0x0000;
	cdef.flags = DoRed|DoGreen|DoBlue;
	XStoreColor(dpy_,cmap,&cdef);
	cdef.pixel = color2;
	cdef.red = 0x0000;
	cdef.blue = 0x0000;
	cdef.green = 0xFFFF;
	cdef.flags = DoRed|DoGreen|DoBlue;
	XStoreColor(dpy_,cmap,&cdef);
    }
    else{
	color0 = color1 = 0;
	color2 = 1;
    }
    pm = XCreateBitmapFromData(dpy_,DefaultRootWindow(dpy_),vmf_bits,
			       vmf_width,vmf_height );
    gcv.function = GXcopy;
    gcv.subwindow_mode = IncludeInferiors;
    gcv.foreground = color1;
    gcv.background = color2;
    gc = XCreateGC(dpy_,DefaultRootWindow(dpy_),
		   GCFunction|GCForeground|GCBackground|
		   GCSubwindowMode,
		   &gcv);
    newWindow.background_pixel = color0;
    if((w = XCreateWindow(dpy_,DefaultRootWindow(dpy_),
			  40,40,
			  360,240,2,0,
			  InputOutput,CopyFromParent,
			  CWBackPixel|CWBorderPixel|CWOverrideRedirect
			  |CWDontPropagate,
			  &newWindow)) < 0L){
	fprintf(stderr,"can not open window\n");
	exit(1);
    }
    XMapWindow(dpy_,w);
    XSelectInput(dpy_,w,ButtonPressMask|ExposureMask);
    XFlush(dpy_);
    while(1){
	XNextEvent(dpy_,&event);
	if(event.type == ButtonPress){
	    break;
	}
	else if(event.type == Expose){
	    XCopyPlane(dpy_, pm, w, gc,
		       0, 0, vmf_width, vmf_height,
		       10, 10, 1);
	    XFlush(dpy_);
	}
    }
    exit(0);
}




