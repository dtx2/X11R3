        DEFINES =  -DTCPCONN -DUNIXCONN -DBINDIR=\"$(BINDIR)\"
LOCAL_LIBRARIES = $(XLIB)
          SRCS1 = xinit.c
          OBJS1 = xinit.o
  SAMPLECONFIGS = xinitrc
       PROGRAMS = xinit startx $(SAMPLECONFIGS)

 OBJS = $(OBJS1) $(OBJS2) $(OBJS3)
 SRCS = $(SRCS1) $(SRCS2) $(SRCS3)

all:: $(PROGRAMS)

xinit: $(OBJS1) $(XLIB)
	$(RM) $@
	$(CC) -o $@ $(OBJS1) $(XLIB) $(LDFLAGS)  $(SYSLAST_LIBRARIES)

install:: xinit
	$(INSTALL) -c $(INSTALLFLAGS) xinit $(BINDIR)

install.man:: xinit.man
	$(INSTALL) -c $(INSTMANFLAGS) xinit.man $(MANDIR)/xinit.n

relink::
	$(RM) $(PROGRAMS)
	$(MAKE) $(MFLAGS) $(PROGRAMS)

depend:: $(DEPEND)

depend::
	$(DEPEND) -s "# DO NOT DELETE" -- $(CFLAGS) -- $(SRCS)

$(DEPEND):
	@echo "making $@"; \
	cd $(DEPENDSRC); $(MAKE)

clean::
	$(RM) $(PROGRAMS)

startx:: startx.cpp
	$(RM) $@
	$(CPP) $(STD_CPP_DEFINES)  -DXINITDIR=$(XINITDIR) <$@.cpp | \
	sed -e '/^#  *[0-9][0-9]*  *.*$$/d' >$@
	chmod a+x $@

xinitrc:: xinitrc.cpp
	$(RM) $@
	$(CPP) $(STD_CPP_DEFINES)  -DXINITDIR=$(XINITDIR) <$@.cpp | \
	sed -e '/^#  *[0-9][0-9]*  *.*$$/d' >$@
	chmod a+x $@

install:: startx
	$(INSTALL) -c $(INSTALLFLAGS) startx $(BINDIR)

