	$(INSTALL) -c $(INSTALLFLAGS) README $(LIBDIR)/x10tox11.help

xlibtags: tags
	sed -e 's,	\([^	]*\)	,	$(XLIBSRC)/\1	,' \
		< $(XLIBSRC)/tags | sort -o tags - tags

