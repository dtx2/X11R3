SRCS = 6x10.bdf 6x12.bdf 6x13.bdf 8x13.bdf 8x13B.bdf 9x15.bdf cursor.bdf
OBJS = 6x10.snf 6x12.snf 6x13.snf 8x13.snf 8x13B.snf 9x15.snf cursor.snf
FONTINSTDIR = $(FONTDIR)/misc

all:: $(OBJS) fonts.dir

fonts.dir: $(OBJS)
	$(MKFONTDIR) .

clean::
	$(RM) *.snf fonts.dir

6x10.snf: 6x10.bdf
	$(FC) $(FCFLAGS) $? >$@

6x12.snf: 6x12.bdf
	$(FC) $(FCFLAGS) $? >$@

6x13.snf: 6x13.bdf
	$(FC) $(FCFLAGS) $? >$@

8x13.snf: 8x13.bdf
	$(FC) $(FCFLAGS) $? >$@

8x13B.snf: 8x13B.bdf
	$(FC) $(FCFLAGS) $? >$@

9x15.snf: 9x15.bdf
	$(FC) $(FCFLAGS) $? >$@

cursor.snf: cursor.bdf
	$(FC) $(FCFLAGS) $? >$@

install:: $(OBJS)
	@case '${MFLAGS}' in *[i]*) set +e;; esac; \
	for i in $(OBJS); do \
	(set -x; $(INSTALL) -c $(INSTALLFLAGS) $$i $(FONTINSTDIR)); \
	done

install:: fonts.dir
	$(INSTALL) -c $(INSTINCFLAGS) fonts.dir $(FONTINSTDIR)

install:: fonts.alias
	$(INSTALL) -c $(INSTINCFLAGS) fonts.alias $(FONTINSTDIR)

depend::
	@echo "skipping make depend in bdf/misc"

###########################################################################
# Imake.tmpl common rules for all Makefiles - do not edit

emptyrule::

clean::
	$(RM_CMD) \#*

Makefile:: $(IMAKE)

Makefile:: Imakefile \
	$(IRULESRC)/Imake.tmpl \
	$(IRULESRC)/Imake.rules \
	$(IRULESRC)/site.def \
	$(IRULESRC)/$(MACROFILE)
	-@if [ -f Makefile ]; then \
	echo "$(RM) Makefile.bak; $(MV) Makefile Makefile.bak"; \
	$(RM) Makefile.bak; $(MV) Makefile Makefile.bak; \
	else exit 0; fi
	$(IMAKE_CMD) -DTOPDIR=$(TOP)

$(IMAKE):
	@echo "making $@"; \
	cd $(IMAKESRC); $(MAKE) BOOTSTRAPCFLAGS=$(BOOTSTRAPCFLAGS)

tags::
	$(TAGS) -w *.[ch]
	$(TAGS) -xw *.[ch] > TAGS

