	$(RM) mfbpainv.c

mfbtewhite.o: mfbtegblt.c mfbtegblt.o
	@$(RM) $@ mfbtewhite.c
	$(LN) mfbtegblt.c mfbtewhite.c
	${CC} -DMFBTEGLYPHBLT=mfbTEGlyphBltWhite \
