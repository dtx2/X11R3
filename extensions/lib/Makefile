    STD_DEFINES =
    CDEBUGFLAGS = -O
       INCLUDES = -I$(TOP) -I$(INCLUDESRC) -I$(XLIBSRC) -I../include
           SRCS = XFillZoids.c XBezier.c XPlxVideo.c XTestExt1.c
           OBJS = XFillZoids.o XBezier.o XPlxVideo.o XTestExt1.o
       LINTLIBS = $(LINTXLIB)

.c.o:
	$(RM) $@
	$(CC) -c $(CFLAGS) $*.c

all:: libXext.a

libXext.a: $(OBJS)
	$(RM) $@
	$(AR) $@ $(OBJS)
	$(RANLIB) $@

lintlib:: llib-lXext.ln

llib-lXext.ln: $(SRCS)
	$(RM) $@
	$(LINT) $(LINTLIBFLAG)Xext $(LINTFLAGS) $(SRCS)

install:: libXext.a
	$(INSTALL) -c $(INSTLIBFLAGS) libXext.a $(USRLIBDIR)
	$(RANLIB) $(RANLIBINSTFLAGS) $(USRLIBDIR)/libXext.a

install.ln:: llib-lXext.ln
	$(INSTALL) -c $(INSTLIBFLAGS) llib-lXext.ln $(LINTLIBDIR)

depend:: $(DEPEND)

depend::
	$(DEPEND) -s "# DO NOT DELETE" -- $(CFLAGS) -- $(SRCS)

$(DEPEND):
	@echo "making $@"; \
	cd $(DEPENDSRC); $(MAKE)

lint:
	$(LINT) $(LINTFLAGS) $(SRCS) $(LINTLIBS)
lint1:
	$(LINT) $(LINTFLAGS) $(FILE) $(LINTLIBS)

